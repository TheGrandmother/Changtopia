def fac(x)
  if x == 1
    return 1
  end
  new_x = x - 1
  return x * fac(new_x)
end

def approx(n)
  if n == 0
    return 0
  end
  term = n / fac(n)
  new_n = n - 1
  return term + approx(new_n)
end

